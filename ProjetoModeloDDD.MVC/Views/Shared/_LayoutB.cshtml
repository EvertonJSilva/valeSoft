<!DOCTYPE html>

<html lang="en">
<head>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery") 
    @Scripts.Render("~/bundles/bootstrap")
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    <title>VS Officium</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Content/SiteB/css/bootstrap.css" rel="stylesheet">
    <!--external css-->
    <link href="~/Content/SiteB/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/Content/SiteB/css/style.css" rel="stylesheet">
    <link href="~/Content/SiteB/css/style-responsive.css" rel="stylesheet" />
    <link href="~/Content/SiteB/css/bootstrap-toggle.css" rel="stylesheet"/>
    <link href="~/Content/SiteB/css/bootstrap2-toggle.css" rel="stylesheet"/>


    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg" >
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>
            <!--logo start-->
            <a href="../../home/index" class="logo"><img src="~/Content/SiteB/img/logoUnipsico.png" width="90"></a>
            <!--logo end-->
            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li>   
                           <a @Html.ActionLink("Logout", "index", "Login", null, new { @class = "logout"})</a>
                    </li>
                </ul>
            </div>
        </header>
        
        
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">

                    <li class="centered"><a href="https://vsvalesoft.wixsite.com/vsvalesoft/" target="_blank"><img src="~/Content/SiteB/img/logoValesoft.png" width="60"></a></li>
                    <li class="centered">VS VALESOFT</li> 
                    <li>
                        <a href="/Pacientes/Index/">
                            <i class="fa fa-group"></i>
                            <span>Pacientes</span>
                            </a>
                    </li>
                    <li>
                        <a href="/Liberacoes/Index/">
                            <i class="fa fa-bar-chart-o"></i>
                            <span>Liberações</span>
                        </a>
                    </li>
                     <li>
                        <a href="/Consultas/Index/">
                            <i class="fa fa-calendar"></i>
                            <span>Consultas</span>
                        </a>
                    </li>
                   
                    @if (((int)Session["nivelAcesso"]) != 2)
                    {
                    <li>
                        <a href="/Profissionais/Index/">
                            <i class="fa fa-credit-card"></i>
                            <span>Profissionais</span>
                        </a>
                    </li>
                    }
                    <li>
                        <a href="/Producao/Index/">
                            <i class="fa fa-print"></i>
                            <span>Produção</span>
                        </a>
                    </li>
                    @if (((int)Session["nivelAcesso"]) != 2)
                    {
                     <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-cogs"></i>
                            <span>Configurações</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/TiposProfissional/Index/">Tipos de profissional</a></li>
                            <li><a href="/ValoresConsulta/Index/">Valores da consulta</a></li>
                            <li><a href="/TaxasDoacao/Index/">Taxas de doação</a></li>
                        </ul>
                    </li>
                    }
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper site-min-height">
                <div class="container body-content">
                    @RenderBody()
                    <hr />
                    
                </div>

            </section><! --/wrapper -->
        </section><!-- /MAIN CONTENT -->
        <!--main content end-->
        <!--footer start-->
        <footer>
            <p>&copy; @DateTime.Now.Year - VS Valesoft Soluções em TI</p>
        </footer>
        <!--footer end-->
    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/Content/SiteB/js/jquery.js"></script>
    <script src="~/Content/SiteB/js/jquery.maskedinput.js"</script>
    <script src="~/Content/SiteB/js/bootstrap.min.js"></script>
    <script src="~/Content/SiteB/js/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="~/Content/SiteB/js/jquery.ui.touch-punch.min.js"></script>
    <script class="include" type="text/javascript" src="~/Content/SiteB/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/SiteB/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/SiteB/js/jquery.nicescroll.js" type="text/javascript"></script>
    <link href="~/Content/Gridmvc.css" rel="stylesheet" />
    <script src="~/Scripts/gridmvc.min.js"></script>
    <script src="~/Content/SiteB/js/bootstrap-toggle.js"></script>
    <script src="~/Content/SiteB/js/bootstrap2-toggle.js"></script>
    

    <!--common script for all pages-->
    <script src="~/Content/SIteB/js/common-scripts.js"></script>

    <!--script for this page-->

    <script>
        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

    </script>

    @RenderSection("scripts", required: false)


</body>
</html>
